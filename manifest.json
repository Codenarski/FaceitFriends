{
    "name": "Faceit Friends",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ],
      "permissions": [
        "storage"
      ],
      "browser_action": {
        "default_popup": "popup.html",
        "default_title": "FaceitFriends"
      }
}
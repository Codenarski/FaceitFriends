{
    "name": "Faceit Friends",
    "version": "1.0.5",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["css/popup.css", "css/editFriends.css"],
          "js": ["content.js"]
        }
      ],
      "permissions": [
        "storage"
      ],
      "web_accessible_resources": [
        "css/popup.css",
        "css/editFriends.css"
      ],
      "icons": { 
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
       "128": "icons/icon128.png" 
      },
      "browser_action": {
        "default_popup": "popup.html",
        "default_title": "FaceitFriends"
      }
}